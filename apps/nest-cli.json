{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/apps/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/apps/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/apps",
  "projects": {
    "apps": {
      "type": "application",
      "root": "apps/apps",
      "entryFile": "main",
      "sourceRoot": "apps/apps/src",
      "compilerOptions": {
        "tsConfigPath": "apps/apps/tsconfig.app.json"
      }
    },
    "gateway": {
      "type": "application",
      "root": "apps/gateway",
      "entryFile": "main",
      "sourceRoot": "apps/gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gateway/tsconfig.app.json"
      }
    },
    "user-info": {
      "type": "application",
      "root": "apps/user-info",
      "entryFile": "main",
      "sourceRoot": "apps/user-info/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user-info/tsconfig.app.json"
      }
    },
    "material": {
      "type": "application",
      "root": "apps/material",
      "entryFile": "main",
      "sourceRoot": "apps/material/src",
      "compilerOptions": {
        "tsConfigPath": "apps/material/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "cache": {
      "type": "library",
      "root": "libs/cache",
      "entryFile": "index",
      "sourceRoot": "libs/cache/src",
      "compilerOptions": {
        "tsConfigPath": "libs/cache/tsconfig.lib.json"
      }
    }
  }
}
